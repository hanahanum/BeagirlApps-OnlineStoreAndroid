            android:exported="false" />
        <activity
            android:name=".views.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".views.MainActivity"
            android:exported="false" />
        <activity
            android:name=".views.MainActivitySignUp"
            android:exported="false" />
        <activity
            android:name=".views.MainActivityLogin"
            android:exported="false" />
